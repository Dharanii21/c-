#include <stdio.h>
    //and operator
int main() {
    int a = 10;
    int b = 5;
    
    printf("%d\n",a&b);
    
    //or operator
    
    int c = 10;
    int d = 5;
    
    printf("%d\n",c|d);
    
    //xor operator
    int e = 10;
    int f = 5;
    
    printf("%d\n",e^f);
    
    //not operator
    int g = 10;
    printf("%d\n",~g);
    
    //leftshift operator
    int h = 10;
    printf("%d\n",h<<2);
    
    //rightshift operator
    int i = 10;
    printf("%d",i>>2);
    return 0;
}
