#include <stdio.h>
//post increment
int main() {
   int a = 21;
   
   printf("%d\n",a++);
   printf("%d\n",a);
   
   //post decrement
   int b = 21;
   printf("%d\n",b--);
   printf("%d\n",b);
   
   //pre increment
   int c = 21;
   printf("%d\n",++c);
   printf("%d\n",c);
   
   //post increment
   int d = 21;
   printf("%d\n",--d);
   printf("%d\n",d);
   return 0;
}
