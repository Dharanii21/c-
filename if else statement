#include <stdio.h>

int main() {
    // Write C code here
    int age=21;
    if(age>18)
    {
        printf("eligible to vote");
    }
    else
    {
        printf("not eligible to vote");
    }
  
    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=3;
    if(num>5)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=6;
    if(num>5 && num<10)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=6;
    if(num%2==0 && num%3==0)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}



#include <stdio.h>

int main() {
    // Write C code here
    int num=11;
    if(num%2==0)
    {
        printf("even\n");
    }
    else{
        printf("odd\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=-216;
    if(num>0)
    {
        printf("positive\n");
    }
    else{
        printf("negative\n");
    }

    return 0;
}



#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("enter the value:");
    scanf("%d",&a);
    if(a>0)
    {
        printf("the no is posi\n");
    }
    else{
        printf("the no is neg\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    float a=;
    float b;
    printf("enter the amount:");
    scanf("%f",&b);
    if(a>b)
    {
        printf("the balance is sufficent\n");
    }
    else{
        printf("the balance is insufficient\n");
    }

    return 0;
}
