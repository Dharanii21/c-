#include <stdio.h>

int main() {
    // Write C code here
    int age=21;
    if(age>18)
    {
        printf("eligible to vote");
    }
    else
    {
        printf("not eligible to vote");
    }
  
    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=3;
    if(num>5)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=6;
    if(num>5 && num<10)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int num=6;
    if(num%2==0 && num%3==0)
    {
        printf("success\n");
    }
    else{
        printf("fail\n");
    }

    return 0;
}



#include <stdio.h>

int main() {
    // Write C code here
    int num=11;
    if(num%2==0)
    {
        printf("even\n");
    }
    else{
        printf("odd\n");
    }

    return 0;
}
