#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("age of the person:");
    scanf("%d",&a);
    if(a<=13){
    
        printf("under 13\n");
    }
    else if(a>14&&a<=60){
        printf("adult\n");
    }
    else{
        printf("senior\n");
    }
    
    
    

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("number::");
    scanf("%d",&a);
    if(a%2==0 && a%3==0){
    
        printf("tamilnadu\n");
    }
    else if(a%2==0){
        printf("andhra\n");
    }
    else if(a%3==0){
        printf("kerala\n");
    }
    else{
        printf("karnataka");
    }
    
    
    

    return 0;
}   




#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("amount:");
    scanf("%d",&a);
    if(a<=1000){
    
        printf("no dis\n");
    }
    else if(a>1000&& a<=5000){
        printf("20%% dis\n");
    }
    else if(a>5000&&a<10000){
        printf("50%% dis\n");
    }
    else{
        printf("70%% dis ");
    }
    
    
    

    return 0;
}   
