#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("age of the person:");
    scanf("%d",&a);
    if(a<=13){
    
        printf("under 13\n");
    }
    else if(a>14&&a<=60){
        printf("adult\n");
    }
    else{
        printf("senior\n");
    }
    
    
    

    return 0;
}


#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("number::");
    scanf("%d",&a);
    if(a%2==0 && a%3==0){
    
        printf("tamilnadu\n");
    }
    else if(a%2==0){
        printf("andhra\n");
    }
    else if(a%3==0){
        printf("kerala\n");
    }
    else{
        printf("karnataka");
    }
    
    
    

    return 0;
}   




#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("amount:");
    scanf("%d",&a);
    if(a<=1000){
    
        printf("no dis\n");
    }
    else if(a>1000&& a<=5000){
        printf("20%% dis\n");
    }
    else if(a>5000&&a<10000){
        printf("50%% dis\n");
    }
    else{
        printf("70%% dis ");
    }
    
    
    

    return 0;
}   




#include <stdio.h>

int main() {
    // Write C code here
    char a;
    printf("character:");
    scanf("%c",&a);
    if(a>='A'&&a<='Z'){
    
        printf("upper case\n");
    }
    else if(a>='a'&&a<='z'){
        printf("lower case\n");
    }
    else if(a>='0'&&a<='9'){
        printf("digits\n");
    }
    else{
        printf("special symbol \n");
    
    }
    
    
    

    return 0;
}   


#include <stdio.h>

int main() {
    // Write C code here
    int car;
    printf("car num:");
    scanf("%d",&car);
    
    
    if(243%car==0){
    
        printf("road a\n");
    }
    else if(135%car==0){
        printf("road b\n");
    }
    else if(567%car==0){
        printf("road c\n");
    }
    else{
        printf("no road\n");
    
    }
    
    
    

    return 0;
}   
